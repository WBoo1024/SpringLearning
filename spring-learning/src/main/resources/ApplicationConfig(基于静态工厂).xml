<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="student" class="lanou.factory.FactoryBean" factory-method="getStudent"/>
    <bean id="clazz" class="lanou.factory.FactoryBean" factory-method="getClazz"/>
    <bean id="course" class="lanou.factory.FactoryBean" factory-method="getCourse"/>
    <bean id="teacher" class="lanou.factory.FactoryBean" factory-method="getTeacher"/>
</beans>